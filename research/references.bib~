@Article{analytic-families,
  Title                    = {Some Consequences of $(V = L)$ in the Theory of Analytic Sets},
  Author                   = {R. W. Hansell},
  Journal                  = {Proceedings of the American Mathematical Society},
  Year                     = {1980},

  Month                    = {oct},
  Number                   = {2},
  Pages                    = {311--319},
  Volume                   = {80},

  Abstract                 = {Following G. M. Reed's definition of a $Q$-set, we define a $Q_A$-set to be any non-$\sigma$-discrete topological space with the property that each subset is (relatively) analytic (= Souslin-$\mathscr{F}$ set). Clearly every $Q$-set is a $Q_A$-set. The discrete irrational extension of the space of real numbers is an example of a first countable hereditarily paracompact (Hausdorff) $Q_A$-set which is not a $Q$-set. THEOREM. $(V = L)$ Let $X$ be a first countable normal space all of whose subsets are analytic. Then $X$ is $\sigma$-discrete if and only if the product of $X$ with the space of irrational numbers is normal. A new structural property of analytic sets is developed in order to utilize a proof technique due to Reed. Several corollaries are obtained on properties of completely additive-analytic families of sets in general metric spaces.},
  Copyright                = {Copyright 1980 American Mathematical Society},
  ISSN                     = {0002-9939},
  Jstor_articletype        = {Full Length Article},
  Jstor_date               = {198010},
  Jstor_formatteddate      = {Oct., 1980},
  Keywords                 = {54H05, 02K05, 04A15, 54E35, 54A25, $(V = L)$, analytic set, $\sigma$-discrete, $Q$-set, $Q_A$-set, completely additive-analytic family, $\sigma$-discretely decomposable, $d$-family, metrizable space},
  Publisher                = {American Mathematical Society},
  Url                      = {http://links.jstor.org/sici?sici=0002-9939%28198010%2980%3A2%3C311%3ASCOITT%3E2.0.CO%3B2-B}
